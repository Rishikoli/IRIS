# API Keys for Real-Time Data Integration
FMP_API_KEY=demo
GEMINI_API_KEY=your_gemini_api_key_here

# Feature Flags for Real-Time Data
USE_REAL_TRENDS=false
USE_REAL_SCRAPING=false
USE_REAL_FMP=false

# Database Configuration
DATABASE_URL=sqlite:///./iris_regtech.db

# Cache Settings
REDIS_URL=redis://localhost:6379
CACHE_TTL_SECONDS=300

# Rate Limiting
FMP_RATE_LIMIT_PER_MINUTE=60
TRENDS_RATE_LIMIT_PER_HOUR=100
SCRAPING_DELAY_SECONDS=1

# Monitoring and Logging
LOG_LEVEL=INFO
ENABLE_METRICS=true